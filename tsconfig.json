{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": "./src",
    "paths": {
      "@layout/*": [
        "layout/*"
      ],
      "@component/*": [
        "component/*"
      ],
      "@lib/*": [
        "lib/*"
      ],
      "@scripts/*": [
        "scripts/*"
      ],
      "@template/*": [
        "template/*"
      ],
      "@state/*": [
        "state/*"
      ],
      "@styles/*": [
        "styles/*"
      ],
      "@styles/layout/*": [
        "styles/Layout/*"
      ],
      "@styles/project/*": [
        "styles/Object/Project/*"
      ],
      "@styles/component/*": [
        "styles/Object/Component/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.module.scss"
, "src/lib/setSitemap.ts", "src/lib/contentful/exportContent/urlSet.js"  ],
  "exclude": [
    "node_modules"
  ]
}